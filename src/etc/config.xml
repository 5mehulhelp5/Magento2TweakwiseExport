<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <tweakwise>
            <export>
                <enabled>0</enabled>
                <key />
                <validate>0</validate>
                <real_time>0</real_time>
                <api_server_url>https://navigator-api.tweakwise.com/task/trigger/</api_server_url>
                <api_import_key />
                <stock_calculation>sum</stock_calculation>
                <product_selection>4</product_selection>
                <out_of_stock_children>1</out_of_stock_children>
                <exclude_child_attributes>url_key,small_image,small_image_label,thumbnail_label,status,tax_class_id</exclude_child_attributes>
            </export>
        </tweakwise>
    </default>
</config>
