{
    "name": "emico/tweakwise-export",
    "license": "OSL-3.0",
    "description": "Magento 2 module for Tweakwise export",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~7.0|~7.1"
    },
    "require-dev": {
        "malukenho/docheader": "~0.1",
        "phpro/grumphp": "^0.11.1",
        "codeception/codeception": "^2.3",
        "codeception/mockery-module": "^0.2.2",
        "league/flysystem": "^1.0"
    },
    "type": "magento2-module",
    "scripts": {
        "docheader": "docheader check ./src"
    },
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "Emico\\TweakwiseExport\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ]
}
